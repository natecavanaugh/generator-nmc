{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {<% if (coverage) { %>
    "test": "gulp test-coverage",
    "coveralls": "gulp coveralls"<% } else { %>
    "test": "mocha" <% } %>
  },
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",
    "bin",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.0.0"
  <% } %>},
  "devDependencies": {
    "mocha": "*"<% if (coverage) { %>,
    "chai": "*",
    "chai-string": "*",
    "coveralls": "*",
    "gulp-coveralls": "*",
    "gulp-istanbul": "*",
    "istanbul": "*",
    "sinon": "*"<% } %><% if (gulp) { %>,
    "gulp": "*",
    "gulp-load-plugins": "*",
    "gulp-help": "*",
    "run-sequence": "*",
    "gulp-mocha": "*"<% } %>
  }
}